{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"group": "build",
			"problemMatcher": "$esbuild-watch",
			"isBackground": true,
			"label": "npm: watch",
			"options": {
				"silent": true
			}
		},
		{
			"type": "npm",
			"script": "build",
			"group": "build",
			"problemMatcher": "$esbuild",
			"label": "npm: build"
		}
	]
}
